    // The following will make VS Code's integrated terminal launch w/ the correct
    // environment.  This way you can launch (as admin) from a shortcut instead of
    // having to launch it from within your office prompt.
    "terminal.integrated.profiles.windows": {
        "PowerShell": {
            "source": "PowerShell",
            "icon": "terminal-powershell"
        },
        "Command Prompt": {
            "name": "Office",
            "path": [
                "${env:windir}\\Sysnative\\cmd.exe",
                "${env:windir}\\System32\\cmd.exe"
            ],
            "args": [
                "/k",
                "init.cmd"
            ],
            "icon": "terminal-cmd"
        },
        "Git Bash": {
            "source": "Git Bash"
        }
    },
    "C_Cpp.default.defines": [
